<%= simple_form_for @quotation_header_history, :html => { :class => 'well form-horizontal' } do |f| %>
  <%= f.input :issue_date , input_html:{:tabindex=>"0"} %>
  <!--<%= f.input :quotation_header_id %>-->
  <%= f.input :construction_name , input_html:{:tabindex=>"1"} %>
  <%= f.input :quote_price , input_html:{:tabindex=>"2"} %>
  <%= f.input :execution_amount , input_html:{:tabindex=>"3"} %>
  <%= f.input :net_amount , input_html:{:tabindex=>"4"} %>
  <!--<%= f.input :last_line_number , input_html:{:tabindex=>"5"} %>-->
  
  <%= f.input :quotation_header_id, input_html:{ name: "quotation_header_id" }, as: :hidden %>
  
  <%= f.button :submit, :class => 'btn-primary',  :tabindex => "5"  %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                quotation_header_histories_path(quotation_header_id: params[:quotation_header_id]), :class => 'btn btn-white_rsd' %>
<% end %>
