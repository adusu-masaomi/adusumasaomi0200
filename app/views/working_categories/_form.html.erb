<%= simple_form_for @working_category, :html => { :class => 'well form-horizontal' } do |f| %>
  <%= f.input :category_name %>
 
  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                working_categories_path, :class => 'btn btn-default' %>
<% end %>
