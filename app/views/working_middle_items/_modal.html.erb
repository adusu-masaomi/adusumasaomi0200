		   //material_id
           material_id = $(this)[0].value;
		   
		   var child_index = $(this)[0].id;
           child_index = child_index.replace( /working_small_item_id/g , "" ) ;
		   
		   if (material_id != undefined){
			 
			 material_id =  parseInt(material_id);
			 
