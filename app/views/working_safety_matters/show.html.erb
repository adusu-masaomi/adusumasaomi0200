<%- model_class = WorkingSafetyMatter -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:working_safety_matter_name) %>:</strong></dt>
  <dd><%= @working_safety_matter.working_safety_matter_name %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              working_safety_matters_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_working_safety_matter_path(@working_safety_matter), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              working_safety_matter_path(@working_safety_matter),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
