<p id="notice"><%= notice %></p>

<h1>Listing Quotation Material Details</h1>

<table>
  <thead>
    <tr>
      <th>Quotation material header</th>
      <th>Material</th>
      <th>Material code</th>
      <th>Material name</th>
      <th>Maker</th>
      <th>Maker name</th>
      <th>Quantity</th>
      <th>Unit master</th>
      <th>List price</th>
      <th>Mail sent flag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotation_material_details.each do |quotation_material_detail| %>
      <tr>
        <td><%= quotation_material_detail.quotation_material_header_id %></td>
        <td><%= quotation_material_detail.material_id %></td>
        <td><%= quotation_material_detail.material_code %></td>
        <td><%= quotation_material_detail.material_name %></td>
        <td><%= quotation_material_detail.maker_id %></td>
        <td><%= quotation_material_detail.maker_name %></td>
        <td><%= quotation_material_detail.quantity %></td>
        <td><%= quotation_material_detail.unit_master_id %></td>
        <td><%= quotation_material_detail.list_price %></td>
        <td><%= quotation_material_detail.mail_sent_flag %></td>
        <td><%= link_to 'Show', quotation_material_detail %></td>
        <td><%= link_to 'Edit', edit_quotation_material_detail_path(quotation_material_detail) %></td>
        <td><%= link_to 'Destroy', quotation_material_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

