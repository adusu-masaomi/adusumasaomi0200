<%= simple_form_for(@quotation_material_detail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :quotation_material_header_id %>
    <%= f.input :material_id %>
    <%= f.input :material_code %>
    <%= f.input :material_name %>
    <%= f.input :maker_id %>
    <%= f.input :maker_name %>
    <%= f.input :quantity %>
    <%= f.input :unit_master_id %>
    <%= f.input :list_price %>
    <%= f.input :mail_sent_flag %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
