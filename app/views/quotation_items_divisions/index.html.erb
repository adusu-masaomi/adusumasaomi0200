<p id="notice"><%= notice %></p>

<h1>Listing Quotation Items Divisions</h1>

<table>
  <thead>
    <tr>
      <th>Quotation items division name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotation_items_divisions.each do |quotation_items_division| %>
      <tr>
        <td><%= quotation_items_division.quotation_items_division_name %></td>
        <td><%= link_to 'Show', quotation_items_division %></td>
        <td><%= link_to 'Edit', edit_quotation_items_division_path(quotation_items_division) %></td>
        <td><%= link_to 'Destroy', quotation_items_division, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

