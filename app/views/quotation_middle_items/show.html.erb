<%- model_class = QuotationMiddleItem -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:quotation_middle_item_name) %>:</strong></dt>
  <dd><%= @quotation_middle_item.quotation_middle_item_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:quotation_middle_item_short_name) %>:</strong></dt>
  <dd><%= @quotation_middle_item.quotation_middle_item_short_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:quotation_middle_specification) %>:</strong></dt>
  <dd><%= @quotation_middle_item.quotation_middle_specification %></dd>
  <dt><strong><%= model_class.human_attribute_name(:quotation_unit_id) %>:</strong></dt>
  <dd><%= @quotation_middle_item.quotation_unit_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:quotation_unit_price) %>:</strong></dt>
  <dd><%= @quotation_middle_item.quotation_unit_price %></dd>
  <dt><strong><%= model_class.human_attribute_name(:quotation_material_name) %>:</strong></dt>
  <dd><%= @quotation_middle_item.quotation_material_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:material_unit_price) %>:</strong></dt>
  <dd><%= @quotation_middle_item.material_unit_price %></dd>
  <dt><strong><%= model_class.human_attribute_name(:labor_unit_price) %>:</strong></dt>
  <dd><%= @quotation_middle_item.labor_unit_price %></dd>
  <dt><strong><%= model_class.human_attribute_name(:labor_productivity_unit) %>:</strong></dt>
  <dd><%= @quotation_middle_item.labor_productivity_unit %></dd>
  <dt><strong><%= model_class.human_attribute_name(:material_quantity) %>:</strong></dt>
  <dd><%= @quotation_middle_item.material_quantity %></dd>
  <dt><strong><%= model_class.human_attribute_name(:accessory_cost) %>:</strong></dt>
  <dd><%= @quotation_middle_item.accessory_cost %></dd>
  <dt><strong><%= model_class.human_attribute_name(:material_cost_total) %>:</strong></dt>
  <dd><%= @quotation_middle_item.material_cost_total %></dd>
  <dt><strong><%= model_class.human_attribute_name(:labor_cost_total) %>:</strong></dt>
  <dd><%= @quotation_middle_item.labor_cost_total %></dd>
  <dt><strong><%= model_class.human_attribute_name(:other_cost) %>:</strong></dt>
  <dd><%= @quotation_middle_item.other_cost %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              quotation_middle_items_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_quotation_middle_item_path(@quotation_middle_item), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              quotation_middle_item_path(@quotation_middle_item),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
