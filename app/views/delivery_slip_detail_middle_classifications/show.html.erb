<%- model_class = DeliverySlipDetailMiddleClassification -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:delivery_slip_header_id) %>:</strong></dt>
  <dd><%= @delivery_slip_detail_middle_classification.delivery_slip_header_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:delivery_slip_detail_large_classification_id) %>:</strong></dt>
  <dd><%= @delivery_slip_detail_middle_classification.delivery_slip_detail_large_classification_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:working_middle_item_id) %>:</strong></dt>
  <dd><%= @delivery_slip_detail_middle_classification.working_middle_item_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:working_middle_item_name) %>:</strong></dt>
  <dd><%= @delivery_slip_detail_middle_classification.working_middle_item_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:quantity) %>:</strong></dt>
  <dd><%= @delivery_slip_detail_middle_classification.quantity %></dd>
  <dt><strong><%= model_class.human_attribute_name(:working_unit_id) %>:</strong></dt>
  <dd><%= @delivery_slip_detail_middle_classification.working_unit_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:delivery_slip_price) %>:</strong></dt>
  <dd><%= @delivery_slip_detail_middle_classification.delivery_slip_price %></dd>
  <dt><strong><%= model_class.human_attribute_name(:execution_price) %>:</strong></dt>
  <dd><%= @delivery_slip_detail_middle_classification.execution_price %></dd>
  <dt><strong><%= model_class.human_attribute_name(:labor_productivity_unit) %>:</strong></dt>
  <dd><%= @delivery_slip_detail_middle_classification.labor_productivity_unit %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              delivery_slip_detail_middle_classifications_path(delivery_slip_header_id: params[:delivery_slip_header_id], delivery_slip_detail_large_classification_id: params[:delivery_slip_detail_large_classification_id]), :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_delivery_slip_detail_middle_classification_path(@delivery_slip_detail_middle_classification), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              delivery_slip_detail_middle_classification_path(@delivery_slip_detail_middle_classification),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
