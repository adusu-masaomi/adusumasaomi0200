<p id="notice"><%= notice %></p>

<h1>Listing Purchase Order Histories</h1>

<table>
  <thead>
    <tr>
      <th>Purchase order date</th>
      <th>Supplier master</th>
      <th>Purchase order datum</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @purchase_order_histories.each do |purchase_order_history| %>
      <tr>
        <td><%= purchase_order_history.purchase_order_date %></td>
        <td><%= purchase_order_history.supplier_master_id %></td>
        <td><%= purchase_order_history.purchase_order_datum_id %></td>
        <td><%= link_to 'Show', purchase_order_history %></td>
        <td><%= link_to 'Edit', edit_purchase_order_history_path(purchase_order_history) %></td>
        <td><%= link_to 'Destroy', purchase_order_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Purchase order history', new_purchase_order_history_path %>
